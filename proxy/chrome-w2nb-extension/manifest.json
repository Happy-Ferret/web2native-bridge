{
  "name": "Web2Native Bridge proxy",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Chrome Extension for emulating the Web2Native Bridge scheme",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [{
      "matches": ["*://*/*", "file:///*"],
      "js": ["content.js"],
      "run_at": "document_end"
  }],
  "background": {
      "scripts": ["extension.js"]
  },
  "web_accessible_resources": ["inject.js"],
  "permissions": ["nativeMessaging"]
}
